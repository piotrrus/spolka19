{
     "name": "spolka19",
     "version": "0.0.0",
     "scripts": {
          "ng": "ng",
          "start": "ng serve",
          "build": "ng build",
          "watch": "ng build --watch --configuration development",
          "test": "ng test",
          "eslint_fix": "npx eslint src --fix",
          "lint": "ng lint",
          "prepare": "husky install"
     },
     "lint-staged": {
          "*.{js,ts}": [
               "eslint --fix",
               "prettier --write"
          ]
     },
     "husky": {
          "hooks": {
               "pre-commit": "npx pretty-quick --staged && npm run lint",
               "pre-push": "ng build --aot true"
          }
     },
     "private": true,
     "dependencies": {
          "@angular/animations": "^19.0.0",
          "@angular/cdk": "^19.2.19",
